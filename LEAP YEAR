#include <stdio.h>

int main()
{
    unsigned int y;
    int ly=0;
    printf("Enter YEAR:");
    scanf("%i",&y);
    if(y%100==0&&y%400==0)
        ly=1;
    else if(y%100!=0&&y%4==0)
        ly=1;
    if(ly==1)
       printf("IT IS LEAP YEAR");
    else
       printf("IT IS NOT LEAP YEAR");

    return 0;
}
