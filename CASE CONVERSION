#include <iostream>
#include <cstring>
#include <cctype>
using namespace std;

int main()
{
    char s[10];
    
    cout<<"ENTER THE STRING-";
    cin>>s;
    cout<<"CASE CONVERTED STRING-"<<endl;
    for(int i=0;i<strlen(s);i++)
    {
        if(islower(s[i]))
         cout<<(char)toupper(s[i]);
        else
         cout<<(char)tolower(s[i]);
    }
    
    return 0;
}
